<body data-no-turbolink="true">
<h1>Make someone new</h1>

<div class="userlist">
<%= form_for(@user) do |f| %> 
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.submit "Create", :class => "button", :id => "special" %>
</div>
    
<% end %>
